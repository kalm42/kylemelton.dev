{
  "extends": ["airbnb", "eslint-config-prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "parser": "babel-eslint",
  "plugins": ["cypress"],
  "env": { "cypress/globals": true },
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/button-has-type": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}
